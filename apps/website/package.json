        "test:unit": "vitest",
        "lint": "prettier --plugin-search-dir . --check . && eslint .",
        "format": "prettier --plugin-search-dir . --write ."
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/project-kiosk/kiosk.git"
    },
    "keywords": [
        "kiosk",
        "project",
        "kiosk",
        "ebooks",
        "ebook",
        "management",
        "webapp",
        "svelte",
        "sveltekit",
        "website",
        "landing",
        "page"
    ],
    "author": {
        "name": "Moritz Friedrich",
        "email": "m@9dev.de"
    },
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/project-kiosk/kiosk/issues"
    },
    "homepage": "https://github.com/project-kiosk/kiosk#readme",
    "devDependencies": {
        "tailwindcss": "^3.2.4",
        "ts-node": "^10.9.1",
        "tslib": "^2.4.1",
        "typescript": "^4.9.3",
        "vite": "^4.0.0",
        "vitest": "^0.25.3",
        "svelte-check": "^2.9.2",
        "@typescript-eslint/eslint-plugin": "^5.45.0",
        "@typescript-eslint/parser": "^5.45.0",
        "autoprefixer": "^10.4.13",
        "cssnano": "^5.1.14",
        "eslint": "^8.28.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-svelte3": "^4.0.0",
        "postcss": "^8.4.20",
        "postcss-import": "^15.1.0",
        "postcss-nesting": "^10.2.0",
        "prettier": "^2.8.0",
        "prettier-plugin-svelte": "^2.8.1",
        "@sveltejs/adapter-auto": "^1.0.0",
        "@sveltejs/kit": "^1.3.10"
    }

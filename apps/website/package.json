    "description": "Website for Project Kiosk",
    "main": "dist/index.es.js",
    "type": "module",
    "scripts": {
        "dev": "vite dev",
        "buildd": "vite build",
        "preview": "vite preview",
        "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
